// 1. Write a Swift program to compute and return the absolute difference of n and 51, if n is over 51 return double the absolute difference.

func absoluteDiff (n: Int) -> Int {
  
 if n > 51 
     {
        return (n - 51) * 2
     }  else {
        return 51 - n
     }
}
print(absoluteDiff(n:300))
